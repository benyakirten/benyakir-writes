{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["DOM"],
    "jsx": "preserve",
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@Blocks": ["src/components/Blocks/index.ts"],
      "@Gen": ["src/components/General/index.ts"],
      "@Input": ["src/components/Input/index.ts"],
      "@Draggable": ["src/components/Input/Draggable/index.ts"],
      "@Pre": ["src/components/Prefix/index.ts"],
      "@Posts": ["src/components/Posts/index.ts"],
      "@Layout": ["src/components/Layout/index.ts"],
      "@Variants": ["src/components/Variants/index.ts"],
      "@Styles/*": ["src/styles/*"],
      "@StyleVars": ["src/styles/variables.ts"],
      "@Animations": ["src/styles/animations.ts"],
      "@Utils/*": ["src/utils/*"],
      "@Images/*": ["src/images/*"],
      "@Tech/*": ["src/images/tech/*"],
      "@Constants": ["src/data/constants.ts"],
      "@Hooks": ["src/hooks/index.ts"],
      "@Data/*": ["src/data/*"],
      "@WPData/*": ["src/data/wp/*"],
      "@Types/*": ["src/types/*"],
      "@Store/*": ["src/store/*"],
      "@TestProps": ["__tests__/props.ts"],
      "@TestUtils": ["__tests__/test-utils.tsx"]
    }, 
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
